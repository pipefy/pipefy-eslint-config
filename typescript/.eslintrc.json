{
  "extends": [
    "react-app",
    "plugin:jest/recommended",
    "eslint-config-recommended",
    "@typescript-eslint/recommended"
  ],
  "plugins": [
    "jest",
    "react",
    "import",
    "prettier",
    "@typescript-eslint/eslint-plugin"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "react/no-unsafe": [2, { "checkAliases": true }]
  }
}
